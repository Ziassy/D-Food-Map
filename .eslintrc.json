{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "operator-linebreak": "off",
        "implicit-arrow-linebreak": "off",
        "no-console": "off",
        "consistent-return": "off",
        "no-prototype-builtins": "off",
        "import/prefer-default-export": "off",
        "no-return-assign": "off"
    },
    "plugins": [
        "jasmine"
    ]
}